{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
 
      "Default": "Warning"
    }
  },
 
"AllowedHosts": "*"
,

  "ConnectionStrings": {
    "DefaultConnection": "#{ConnectionString}"
  },

  "Jwt": {
    "Key": "#{JwtKey}",
    "Issuer": "#{JwtIssuer}",
    "Audience": "Any",
    "AccessTokenExpirationMinutes": 2,
    "RefreshTokenExpirationMinutes": 60,
    "AllowMultipleLoginsFromTheSameUser": false,
    "AllowSignoutAllUserActiveClients": true
  }
}
 